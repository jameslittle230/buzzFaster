{
    "manifest_version": 2,

    "name": "Bowdoin Hackathon Extension",
    "description": "",
    "version": "1.1",

    "content_scripts": [{
        "matches": ["*://*.buzzfeed.com/*"],
        "css": ["main.css", "nprogress.css"],
        "js": ["jquery.js", "nprogress.js", "main.js"]
    }],
    "permissions": [
        "tabs", "http://www.google.com/*"
    ],
    "web_accessible_resources": [
        "camera.svg"
    ]
}
